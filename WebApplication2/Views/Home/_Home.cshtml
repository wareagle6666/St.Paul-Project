<!-- ko with: home -->
<div class="jumbotron">

    <div class="col-md-6 paypal">
        <form action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_blank">
            <input type="hidden" name="cmd" value="_s-xclick" />
            <input type="hidden" name="hosted_button_id" value="UJM2VHZULXGZA" />
            <input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_donateCC_LG.gif" border="0" name="submit" title="PayPal - The safer, easier way to pay online!" alt="Donate with PayPal button" />
            <img alt="" border="0" src="https://www.paypal.com/en_US/i/scr/pixel.gif" width="1" height="1" />
        </form>
    </div>

    <div class="col-md-6 facebook">
        <form action="https://www.facebook.com/groups/199485506854969/" method="post" target="_blank">
            <input type="hidden" name="cmd" value="_s-xclick" />
            <input type="image" src="~/Content/Images/facebook2.jpg" width="147" height="47" />
            <img alt="facebook" border="0" src="~/Content/Images/facebook.jpg" width="1" height="1" />
        </form>
    </div>
</div>


<div class="col-lg-12 ">
    @{ 
           var getimage = Convert.ToBase64String(Model.ImageData);
           var picture = string.Format("data:image/jpeg;base64,{0}", getimage);
           <img src="@Url.Content(picture)" style="width:100%;" />
     }
</div>




